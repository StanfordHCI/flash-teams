<%= form_tag("/members/create", method: "post") do %>
  <%= label_tag(:name, "Your name:") %>
  <%= text_field_tag(:name) %>
  <%= label_tag(:email, "Your email:") %>
  <%= text_field_tag(:email) %>
  <%= hidden_field_tag(:uniq, @uniq) %>
  <%= hidden_field_tag(:flash_team_id, @id) %>
  <%= submit_tag("Submit") %>
<% end %>